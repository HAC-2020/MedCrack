<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.test.medcrack.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_maps"
        android:name="com.example.test.medcrack.MapFragment"
        android:label="@string/nav_maps"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/nav_alerts"
        android:name="com.example.test.medcrack.AlertsFragment"
        android:label="@string/nav_alerts"
        tools:layout="@layout/fragment_alerts" />
    <activity
        android:id="@+id/nav_logout"
        android:name="com.example.test.medcrack.Login"
        android:label="activity_login"
